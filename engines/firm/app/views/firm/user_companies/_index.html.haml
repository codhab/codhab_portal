- @user_companies.each do |u|
  %tr
    %td.col-md-4= u.name
    %td.col-md-2= u.email
    %td.col-md-2= u.login_user
    %td.col-md-2
      = link_to "editar".html_safe, edit_user_company_path(u), remote: true, class: " btn btn-info btn-xs"
      = link_to "excluir".html_safe, user_company_path(u), method: :delete, data: { confirm: t(:confirm), disable_with: t(:disable_with)}, class: " btn btn-danger btn-xs" if @firm.id != u.id
