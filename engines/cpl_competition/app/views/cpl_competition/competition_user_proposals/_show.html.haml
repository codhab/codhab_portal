.modal-header
  %h5.modal-title 
    %b ENVELOPE 2
  %button.close{"aria-label" => "Fechar", "data-dismiss" => "modal", :type => "button"}
    %span{"aria-hidden" => "true"} Ã—
.modal-body
  
  %table.table.table-striped.table-celled.table-bordered
    %thead
      %tr
        %th Data/Hora
        %th Arquivo
    %tbody
      - @proposals.where(deleted: false).each do |pro|
        %tr
          %td= pro.created_at.strftime("%d/%m/%Y %H:%M")
          %td.collapsing= link_to "Visualizar proposta", pro.document_url, target: "_blank"